{
 ;; Increase style attribute frequency
 [:global-attribute :alt 11] 1000,

 ;; Decrease HTML reference and comment frequency
 [:content :alt 1] 5,
 [:content :alt 2] 5,

 ;; Decrease Ahem unicode glyph
 [:char-data :alt 2] 10,

 ;; Decrease CSS comments
 [:css-declaration :alt 1] 5,

 ;; Decrease CSS !important flag
 [:css-declaration :alt 0 :cat 1 :alt 1] 5,
}
